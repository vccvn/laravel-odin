{
    "name": "odin/core",
    "description": "library for Laravel Web3",
    "authors": [
        {
            "name": "Doan LN",
            "email": "developer.vcc@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Odin\\": "src/app/",
            "PHPHtmlParser\\": "src/PHPHtmlParser/"
        },
        "files": [
            "src/helpers/utils.php"
        ]
    },
    "require": {
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "bacon/bacon-qr-code": "^2.0",
        "guzzlehttp/guzzle": "7.x",
        "james-heinrich/getid3": "*",
        "laravel/framework": "^11.9",
        "myclabs/php-enum": "^1.8",
        "paquettg/string-encode": "~1.0.0",
        "phpoffice/phpspreadsheet": "^1.28",
        "phpoffice/phpword": "^0.18.3"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.4-dev"
        },
        "laravel": {
            "providers": [
                "Odin\\Providers\\CoreServiceProvider"
            ]
        }
    }
}
